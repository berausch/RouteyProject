@{
    ViewData["Title"] = "Home Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script>
    $(document).ready(function () {


        $("#auto").autocomplete({
            source: function (request, response) {
                
                $.ajax({
                    url: '@Url.Action("GetAuto")',
                    dataType: "json",
                    data: {
                        term: request.term
                    },
                    success: function (result) {
                        console.log(result);

                        response(result);
                    }
                });
            },
            minLength: 2,
            select: function (event, ui) {
                $.ajax({
                    type: 'GET',
                    dataType: 'html',
                    data: {
                        auto: ui.item.value
                    },
                    url: '@Url.Action("GetLocations")',
                    success: function (result) {
                        console.log(result);
                        $('#log').html(result);
                    }
                });
            }
        });
        $("#auto2").submit(function (event) {
            event.preventDefault();
            $.ajax({
                type: 'GET',
                dataType: 'html',
                data: $(this).serialize(),
                url: '@Url.Action("GetLocations")',
                success: function (result) {
                    $('#log').html(result);
                }
            });
        });
    });
</script>


    <div class="ui-widget">
        <form id="auto2">
            <label for="auto">Write In: </label>
            <input id="auto" name="auto">
            <button type="submit">Submit</button>
        </form>
    </div>

    <div class="ui-widget" style="margin-top:2em; font-family:Arial">
        Results:
        <div id="log"></div>
    </div>




